{
  "name": "cbs-bootstrap",
  "dependencies": {
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.6",
    "@types/jquery": "^3.5.16",
    "better-docs": "^2.7.2",
    "jsdoc": "^4.0.2",
    "typedoc": "^0.24.4"
  },
  "version": "0.0.3",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --out docs src",
    "docs:dev": "better-docs --out docs src",
    "docs:dev:watch": "better-docs --out docs src --watch",
    "docs:watch": "typedoc --out docs src --watch"
  },
  "author": "Taylor King",
  "license": "MIT",
  "description": "A collection of Bootstrap 5 components for use with TypeScript",
  "keywords": [
    "bootstrap",
    "typescript",
    "components",
    "cbs",
    "cbs-bootstrap",
    "custom bootstrap"
  ]
}
